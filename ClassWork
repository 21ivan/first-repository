a=int(input('input a '))
b=int(input('input b '))
if a>=b:
    print ('max=', a)
else:
    print ('min=', b)
###############################
num = int(input('input num'))
if (num%2)==0:
    print("parne")
else:
    print('ne parne')
###############################
num=int(input('input chislo - ' ))
factorial = 1
while num>1:
    factorial*=num
    num-=1
    #print(factorial)
print(factorial)
